{
  "version": "2.0",
  "extensions": {
    "http": {
      "routePrefix": "api",
      "cors": {
        "allowedOrigins": ["*"],
        "allowedMethods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
        "exposedHeaders": ["Authorization"],
        "allowedHeaders": [ "*" ],
        "maxAge": 3600
      }
    }
  }
}
